<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!-- 打开命令口(win+R),在命令窗口中进入当前文件所在目录，执行下面的命令 java -jar mybatis-generator-core-1.3.2.jar  -configfile generator.xml -overwrite -->
<generatorConfiguration>
	<!-- 驱动jar包，mssql使用jtds-1.2.jar,mysql使用mysql-connector-java-5.0.8-bin.jar -->
	<classPathEntry location="./mysql-connector-java-commercial-5.1.25-bin.jar" />
	<context id="FreeCMS_Tables" targetRuntime="MyBatis3">
		<!-- mysql配置 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/db_goods_demo" userId="root"
			password="123456">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- javabean生成文件的存放位置 -->
		<javaModelGenerator targetPackage="com.cdu.bean"
			targetProject="../src">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>


		<!-- sqlmap生成文件的存放位置 -->
		<sqlMapGenerator targetPackage="com.cdu.dao"
			targetProject="../src">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- dao生成文件的存放位置 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.cdu.dao" targetProject="../src">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- 要生成的表 -->
		<table tableName="tb_goods" domainObjectName="Goods"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
	<!-- 	<table tableName="tb_photo" domainObjectName="Photo"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
		<table tableName="tb_goods_type" domainObjectName="GoodsType"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
		<table tableName="tb_standard" domainObjectName="Standard"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table> -->
	

	</context>

</generatorConfiguration>